{
  "dependencyDashboard": false,
  "extends": [
    "config:best-practices",
    "docker:enableMajor",
    ":disableRateLimiting",
    ":semanticCommits",
    ":rebaseStalePrs",
    ":automergePr"
  ],
  "labels": [
    "Type: Renovate"
  ],
  "packageRules": [
    {
      "labels": [
        "Release: Major"
      ],
      "matchUpdateTypes": [
        "major"
      ]
    },
    {
      "labels": [
        "Release: Minor"
      ],
      "matchUpdateTypes": [
        "minor"
      ]
    },
    {
      "labels": [
        "Release: Patch"
      ],
      "matchUpdateTypes": [
        "patch"
      ]
    }
  ],
  "pinDigests": false
}
