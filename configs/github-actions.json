{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "automerge": true,
      "automergeType": "branch",
      "description": [
        "Auto merge Github Actions"
      ],
      "ignoreTests": false,
      "labels": [
        "Release: Skip",
        "Type: Renovate"
      ],
      "matchManagers": [
        "github-actions"
      ],
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch",
        "digest"
      ],
      "pinDigests": false
    }
  ]
}
